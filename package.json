{
  "name": "@quocanhbk17/express-cassandra",
  "version": "0.1.12",
  "description": "Nest - modern, fast, powerful node.js web framework (@express-cassandra)",
  "author": {
    "name": "Fahim Rahman",
    "email": "faim.sust@gmail.com",
    "url": "https://github.com/ifaim"
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "watch": "tsc -w",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ifaim/nestjs-express-cassandra.git"
  },
  "homepage": "https://github.com/ifaim/nestjs-express-cassandra",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "express-cassandra": "^2.9.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.4.6",
    "@nestjs/core": "^10.4.6",
    "@types/jest": "^29.5.14",
    "@types/merge-deep": "^3.0.3",
    "@types/node": "10.12.18",
    "express-cassandra": "^2.9.1",
    "husky": "3.0.9",
    "jest": "^29.7.0",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.2.5",
    "tslint": "5.20.0",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "merge-deep": "^3.0.3"
  },
  "keywords": [
    "nestjs",
    "cassandra",
    "apache-cassandra",
    "datastax",
    "orm",
    "express-cassandra",
    "node",
    "nodejs",
    "javascript"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
